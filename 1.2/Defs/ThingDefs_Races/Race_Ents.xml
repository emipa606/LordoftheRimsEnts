<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <AlienRace.RaceSettings>
    <defName>Ent_RaceSettings</defName>
    <pawnKindSettings>
      <alienrefugeekinds>
        <li>
          <kindDefs>
            <li>EntMelee</li>
          </kindDefs>
          <chance>10.0</chance>
        </li>
      </alienrefugeekinds>
      <alienslavekinds>
        <li>
          <kindDefs>
            <li>EntMelee</li>
          </kindDefs>
          <chance>10.0</chance>
        </li>
      </alienslavekinds>
      <startingColonists>
        <li>
          <pawnKindEntries>
            <li>
              <kindDefs>
                <li>PlayerEnt</li>
              </kindDefs>
              <chance>10.0</chance>
            </li>
          </pawnKindEntries>
          <factionDefs>
            <li>EntFaction</li>
            <li>EntPlayerTribe</li>
          </factionDefs>
        </li>
