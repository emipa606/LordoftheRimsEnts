  <AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>Ent_Race</defName>
    <label>Ent</label>
    <description>A Ent, a mystical guardian of the Forest.</description>
    <alienRace>
      <generalSettings>
        <maleGenderProbability>1.0</maleGenderProbability>
        <immuneToAge>true</immuneToAge>
        <canLayDown>false</canLayDown>
        <disallowedTraits>
          <li>
            <defName>Pyromaniac</defName>
          </li>
        </disallowedTraits>
        <factionRelations>
          <li>
            <factions>
              <li>EntFaction</li>
              <li>EntPlayerTribe</li>
            </factions>
            <goodwill>
              <min>-50</min>
              <max>50</max>
            </goodwill>
          </li>
        </factionRelations>
        <maxDamageForSocialfight>6</maxDamageForSocialfight>
        <allowHumanBios>false</allowHumanBios>
        <immuneToXenophobia>false</immuneToXenophobia>
        <alienPartGenerator>
          <aliencrowntypes>
            <li>Average_Normal</li>
            <li>Narrow_Normal</li>
          </aliencrowntypes>
          <alienbodytypes>
            <li>Fat</li>
            <li>Hulk</li>
          </alienbodytypes>
          <customDrawSize>(2.5,2.5)</customDrawSize>
          <useGenderedHeads>false</useGenderedHeads>
          <useGenderedBodies>false</useGenderedBodies>
          <colorChannels>
            <li>
              <name>skin</name>
              <first Class="ColorGenerator_Options">
                <options>
                  <li>
                    <weight>15</weight>
                    <only>RGBA(0.219,0.18,0.0,1)</only>
                  </li>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.26,0.141,0.007,1)</only>
                  </li>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.219,0.113,0.003,1)</only>
                  </li>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.356,0.188,0.007,1)</only>
                  </li>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.239,0.125,0.003,1)</only>
                  </li>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.129,0.066,0.0,1)</only>
                  </li>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.388,0.203,0.007,1)</only>
                  </li>
                </options>
              </first>
            </li>
          </colorChannels>
        </alienPartGenerator>
      </generalSettings>
      <graphicPaths>
        <li>
          <body>Pawn/Humanlike/Ent/Body/</body>
          <head>Pawn/Humanlike/Ent/Head/</head>
          <headOffset>(0,-0.4)</headOffset>
        </li>
      </graphicPaths>
      <hairSettings>
        <hasHair>false</hasHair>
        <getsGreyAt>10000</getsGreyAt>
      </hairSettings>
      <thoughtSettings>
        <cannotReceiveThoughts>
          <li>Naked</li>
          <li>AteInsectMeatDirect</li>
          <li>AteInsectMeatAsIngredient</li>
          <li>SleptOnGround</li>
          <li>SleptOutside</li>
          <li>SleptInBarracks</li>
          <li>AteRawFood</li>
          <li>AteWithoutTable</li>
          <li>AteAwfulMeal</li>
          <li>ApparelDamaged</li>
        </cannotReceiveThoughts>
      </thoughtSettings>
      <raceRestriction>
        <onlyUseRaceRestrictedApparel>true</onlyUseRaceRestrictedApparel>
        <weaponList>
          <li>Weapon_Boulder</li>
        </weaponList>
      </raceRestriction>
    </alienRace>
    <statBases>
      <ArmorRating_Blunt>0.2</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.2</ArmorRating_Sharp>
      <CarryingCapacity>800</CarryingCapacity>
      <Mass>320</Mass>
      <MoveSpeed>2</MoveSpeed>
      <Flammability>2</Flammability>
      <ComfyTemperatureMin>-80</ComfyTemperatureMin>
      <ComfyTemperatureMax>85</ComfyTemperatureMax>
      <LeatherAmount>0</LeatherAmount>
      <MeatAmount>0</MeatAmount>
      <PsychicSensitivity>.5</PsychicSensitivity>
      <PlantWorkSpeed>1.5</PlantWorkSpeed>
    </statBases>
    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>30</power>
        <cooldownTime>2.4</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>85</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>30</power>
        <cooldownTime>2.4</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>85</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>22</power>
        <cooldownTime>2.0</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.3</chanceFactor>
      </li>
    </tools>
    <race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <nameGenerator>NamerEnts</nameGenerator>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <leatherDef>Leather_Human</leatherDef>
      <lifeExpectancy>10000</lifeExpectancy>
      <nameCategory>HumanStandard</nameCategory>
      <body>EntBody</body>
      <baseBodySize>2.5</baseBodySize>
      <baseHealthScale>5</baseHealthScale>
      <bloodDef>sap</bloodDef>
      <foodType>OmnivoreHuman</foodType>
      <gestationPeriodDays>45</gestationPeriodDays>
      <fleshType>entFlesh</fleshType>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(1.01, 0.02)</li>
          <li>(3.5, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeStageAges>
        <li>
          <def>EntSapling</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>Entling</def>
          <minAge>13</minAge>
        </li>
        <li>
          <def>EntAdult</def>
          <minAge>20</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(14,0)</li>
          <li>(16,100)</li>
          <li>(50,100)</li>
          <li>(60,30)</li>
          <li>(70,18)</li>
          <li>(80,10)</li>
          <li>(90,3)</li>
          <li>(500,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>EntX</li>
      </hediffGiverSets>
    </race>
    <recipes>
      <li>InstallPowerClaw</li>
      <li>InstallBionicEye</li>
      <li>InstallBionicArm</li>
      <li>InstallBionicLeg</li>
      <li>InstallSimpleProstheticArm</li>
      <li>InstallSimpleProstheticLeg</li>
      <li>InstallPegLeg</li>
      <li>InstallDenture</li>
      <li>InstallJoywire</li>
      <li>InstallPainstopper</li>
      <li>InstallNaturalHeart</li>
      <li>InstallNaturalLung</li>
      <li>InstallNaturalKidney</li>
      <li>InstallNaturalLiver</li>
      <li>ExciseCarcinoma</li>
      <li>AdministerMechSerumHealer</li>
      <li>RemoveBodyPart</li>
      <li>Euthanize</li>
      <li>Anesthetize</li>
    </recipes>
    <butcherProducts>
      <EntWood>140</EntWood>
    </butcherProducts>
  </AlienRace.ThingDef_AlienRace>
</Defs>